    {
        "id": "ee31428aef508598",
        "type": "function",
        "z": "842c6d22ccf8aa51",
        "name": "Update state from telemetry",
        "func": "const p = msg.payload || {};\nconst s = flow.get('sm1') || {};\n\ns.telemetryOnline = true;\ns.lastSeenMs = Date.now();\n\ns.alert = !!p.alert;\ns.alertFlags = Number(p.alertFlags ?? 0);\ns.buzzer = !!p.buzzer;\n\n// Parse RSSI and determine signal strength\nconst rssiNum = Number(p.rssi ?? -100);\ns.rssi = (p.rssi !== undefined) ? `${p.rssi} dBm` : '--';\ns.rssiValue = rssiNum;\n\n// Signal strength: Excellent > -50, Good > -60, Fair > -70, Poor <= -70\nif (rssiNum > -50) {\n  s.signalStrength = 'Excellent';\n  s.signalColor = '#2DFFB3'; // green\n  s.signalBars = 4;\n} else if (rssiNum > -60) {\n  s.signalStrength = 'Good';\n  s.signalColor = '#7f00ff'; // purple\n  s.signalBars = 3;\n} else if (rssiNum > -70) {\n  s.signalStrength = 'Fair';\n  s.signalColor = '#FFD166'; // yellow\n  s.signalBars = 2;\n} else {\n  s.signalStrength = 'Poor';\n  s.signalColor = '#ff4d6d'; // red\n  s.signalBars = 1;\n}\n\nconst up = Number(p.uptime ?? 0);\nconst hh = String(Math.floor(up/3600)).padStart(2,'0');\nconst mm = String(Math.floor((up%3600)/60)).padStart(2,'0');\nconst ss = String(Math.floor(up%60)).padStart(2,'0');\ns.uptime = `${hh}:${mm}:${ss}`;\n\ns.temp = Number(p.temp ?? 0);\ns.hum  = Number(p.hum  ?? 0);\ns.dust = Number(p.dust ?? 0);\ns.mq2  = Number(p.mq2  ?? 0);\n\n// Decode alertFlags bitfield\n// bit0=temp, bit1=humidity, bit2=dust, bit3=mq2\nconst flags = s.alertFlags;\nconst alertReasons = [];\nif (flags & 1) alertReasons.push('Temp');\nif (flags & 2) alertReasons.push('Humidity');\nif (flags & 4) alertReasons.push('Dust');\nif (flags & 8) alertReasons.push('Gas');\ns.alertReasons = alertReasons.length ? alertReasons.join(', ') : 'None';\n\nif (!s.lastCommand) s.lastCommand = '--';\nif (!s.mqttLwt) s.mqttLwt = '--';\nif (!s.thresholds) s.thresholds = { tempMin: 18, tempMax: 30, humMin: 30, humMax: 70, dustHigh: 400, mq2High: 60 };\n\nflow.set('sm1', s);\n\n// UI state\nconst ui = { payload: { ...s } };\n\n// Gauges\nconst t1 = { payload: s.temp };\nconst t2 = { payload: s.hum };\nconst t3 = { payload: s.dust };\nconst t4 = { payload: s.mq2 };\n\nreturn [ui, t1, t2, t3, t4];",
