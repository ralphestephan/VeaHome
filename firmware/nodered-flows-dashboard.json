    {
        "id": "3f283f1076d99985",
        "type": "ui_template",
        "z": "842c6d22ccf8aa51",
        "group": "uiGroup_overview_v360",
        "name": "Vealive360 Overview (cards + thresholds)",
        "order": 1,
        "width": "12",
        "height": "12",
        "format": "<style>\n  body.nr-dashboard-theme{\n    background: radial-gradient(1200px 700px at 15% 10%, rgba(255,255,255,0.12), transparent 55%),\n                radial-gradient(900px 600px at 90% 20%, rgba(255,255,255,0.10), transparent 60%),\n                linear-gradient(135deg, #00c6ff 0%, #7f00ff 100%) !important;\n  }\n  .v360-shell{\n    border-radius: 22px;\n    border: 1px solid rgba(255,255,255,.18);\n    background: rgba(0,0,0,.18);\n    box-shadow: 0 18px 50px rgba(0,0,0,.35);\n    backdrop-filter: blur(10px);\n    padding: 16px;\n  }\n  .v360-top{display:flex;align-items:center;justify-content:space-between;gap:12px;margin-bottom:14px;}\n  .v360-brand{display:flex;align-items:center;gap:12px;min-width:0;}\n  .v360-logo{width:44px;height:44px;border-radius:16px;border:1px solid rgba(255,255,255,.22);\n    background: radial-gradient(22px 22px at 30% 25%, rgba(255,255,255,.45), transparent 55%),\n                linear-gradient(135deg, rgba(255,255,255,.22), rgba(255,255,255,.10));\n    box-shadow: 0 14px 30px rgba(0,0,0,.25);\n    position:relative;overflow:hidden;flex:0 0 auto;\n  }\n  .v360-logo:after{content:\"\";position:absolute;inset:-40%;background:linear-gradient(135deg, rgba(0,198,255,.55), rgba(127,0,255,.55));transform:rotate(25deg);}\n  .v360-title{min-width:0;}\n  .v360-title .h{font-size:18px;font-weight:900;line-height:1;background:linear-gradient(90deg,#c7f0ff,#e6d6ff);\n    -webkit-background-clip:text;background-clip:text;color:transparent;white-space:nowrap;overflow:hidden;text-overflow:ellipsis;}\n  .v360-title .p{opacity:.85;font-size:12px;white-space:nowrap;overflow:hidden;text-overflow:ellipsis;color:rgba(255,255,255,.90)}\n  .v360-pill{padding:8px 10px;border-radius:999px;border:1px solid rgba(255,255,255,.22);background:rgba(0,0,0,.10);font-size:12px;color:rgba(255,255,255,.9)}\n\n  .v360-grid{display:grid;gap:12px;grid-template-columns: repeat(4, minmax(0,1fr));}\n  @media (max-width: 960px){ .v360-grid{grid-template-columns: repeat(2, minmax(0,1fr));} }\n  @media (max-width: 520px){ .v360-grid{grid-template-columns: 1fr;} }\n\n  .kpi{border-radius:16px;border:1px solid rgba(255,255,255,.14);background:rgba(255,255,255,.10);padding:12px;min-height:72px;position:relative;}\n  .kpi .t{font-size:12px;opacity:.85;color:rgba(255,255,255,.85);margin-bottom:6px;}\n  .kpi .v{display:flex;align-items:center;gap:10px;font-size:18px;font-weight:900;color:rgba(255,255,255,.95);}\n\n  .dot{width:10px;height:10px;border-radius:50%;}\n  .dot-on{background:#2DFFB3;box-shadow:0 0 0 4px rgba(45,255,179,.12);} \n  .dot-off{background:#ff4d6d;box-shadow:0 0 0 4px rgba(255,77,109,.12);} \n  .dot-warn{background:#FFD166;box-shadow:0 0 0 4px rgba(255,209,102,.12);} \n\n  .iconbtn{position:absolute;right:10px;top:10px;border:none;cursor:pointer;padding:10px;border-radius:14px;\n    background:rgba(0,0,0,.18);border:1px solid rgba(255,255,255,.18);\n  }\n  .iconbtn:active{transform:translateY(1px);}\n  .iconbtn svg{display:block}\n\n  .mono{font-family: ui-monospace, SFMono-Regular, Menlo, Monaco, Consolas, \"Liberation Mono\", \"Courier New\", monospace;}\n  \n  .signal-bars{display:flex;gap:3px;align-items:flex-end;height:20px;}\n  .signal-bar{width:4px;background:rgba(255,255,255,.3);border-radius:2px;transition:all 0.3s;}\n  .signal-bar.active{background:currentColor;}\n  .signal-bar:nth-child(1){height:6px;}\n  .signal-bar:nth-child(2){height:10px;}\n  .signal-bar:nth-child(3){height:14px;}\n  .signal-bar:nth-child(4){height:20px;}\n  \n  .v360-section{margin-top:16px;padding-top:14px;border-top:1px solid rgba(255,255,255,.12);}\n  .v360-section-title{font-size:14px;font-weight:700;color:rgba(255,255,255,.9);margin-bottom:10px;}\n  .threshold-grid{display:grid;gap:8px;grid-template-columns: repeat(3, minmax(0,1fr));}\n  @media (max-width: 640px){ .threshold-grid{grid-template-columns: repeat(2, minmax(0,1fr));} }\n  .threshold-item{background:rgba(255,255,255,.08);border-radius:12px;padding:10px;text-align:center;}\n  .threshold-item .label{font-size:11px;opacity:.7;color:rgba(255,255,255,.8);}\n  .threshold-item .value{font-size:16px;font-weight:700;color:#fff;margin-top:4px;}\n</style>\n\n<div class=\"v360-shell\">\n  <div class=\"v360-top\">\n    <div class=\"v360-brand\">\n      <div class=\"v360-logo\"></div>\n      <div class=\"v360-title\">\n        <div class=\"h\">Vealive360</div>\n        <div class=\"p\">SmartMonitor • Device 1</div>\n      </div>\n    </div>\n    <div class=\"v360-pill\">Broker&nbsp;&nbsp;<span class=\"mono\">63.34.243.171</span></div>\n  </div>\n\n  <div class=\"v360-grid\">\n\n    <div class=\"kpi\">\n      <div class=\"t\">DEVICE (telemetry)</div>\n      <div class=\"v\">\n        <span class=\"dot\" ng-class=\"state.telemetryOnline ? 'dot-on':'dot-off'\"></span>\n        <span>{{state.telemetryOnline ? 'online' : 'offline'}}</span>\n      </div>\n    </div>\n\n    <div class=\"kpi\">\n      <div class=\"t\">MQTT LWT</div>\n      <div class=\"v\">\n        <span class=\"dot\" ng-class=\"(state.mqttLwt==='ONLINE') ? 'dot-on' : (state.mqttLwt==='OFFLINE' ? 'dot-off' : 'dot-warn')\"></span>\n        <span>{{state.mqttLwt || '--'}}</span>\n      </div>\n    </div>\n\n    <div class=\"kpi\">\n      <div class=\"t\">ALERT</div>\n      <div class=\"v\">\n        <span class=\"dot\" ng-class=\"state.alert ? 'dot-off' : 'dot-on'\"></span>\n        <span>{{state.alert ? 'ALERT' : 'OK'}}</span>\n      </div>\n      <div style=\"font-size:11px;color:rgba(255,255,255,.7);margin-top:4px;\" ng-if=\"state.alert\">\n        {{state.alertReasons}}\n      </div>\n    </div>\n\n    <div class=\"kpi\">\n      <div class=\"t\">BUZZER</div>\n      <button class=\"iconbtn\" ng-click=\"toggleBuzzer()\" title=\"Mute / Unmute\">\n        <svg ng-if=\"state.buzzer\" width=\"22\" height=\"22\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"white\" stroke-width=\"2\" stroke-linecap=\"round\" stroke-linejoin=\"round\">\n          <path d=\"M11 5L6 9H2v6h4l5 4V5z\"/>\n          <path d=\"M15.5 8.5a5 5 0 010 7\"/>\n          <path d=\"M17.5 6.5a8 8 0 010 11\"/>\n        </svg>\n        <svg ng-if=\"!state.buzzer\" width=\"22\" height=\"22\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"white\" stroke-width=\"2\" stroke-linecap=\"round\" stroke-linejoin=\"round\">\n          <path d=\"M11 5L6 9H2v6h4l5 4V5z\"/>\n          <path d=\"M23 9l-6 6\"/>\n          <path d=\"M17 9l6 6\"/>\n        </svg>\n      </button>\n      <div class=\"v\">\n        <span class=\"dot\" ng-class=\"state.buzzer ? 'dot-on':'dot-warn'\"></span>\n        <span>{{state.buzzer ? 'ON' : 'MUTED'}}</span>\n      </div>\n    </div>\n\n    <div class=\"kpi\">\n      <div class=\"t\">RSSI (Signal Strength)</div>\n      <div class=\"v\" style=\"justify-content:center;flex-direction:column;gap:6px;\">\n        <div style=\"display:flex;align-items:center;gap:10px;\">\n          <div class=\"signal-bars\" ng-style=\"{color: state.signalColor || '#fff'}\">\n            <div class=\"signal-bar\" ng-class=\"{active: state.signalBars >= 1}\"></div>\n            <div class=\"signal-bar\" ng-class=\"{active: state.signalBars >= 2}\"></div>\n            <div class=\"signal-bar\" ng-class=\"{active: state.signalBars >= 3}\"></div>\n            <div class=\"signal-bar\" ng-class=\"{active: state.signalBars >= 4}\"></div>\n          </div>\n          <span style=\"font-size:14px;\">{{state.signalStrength || 'N/A'}}</span>\n        </div>\n        <div style=\"font-size:11px;opacity:0.7;text-align:center;\"><span class=\"mono\">{{state.rssiValue || '--'}} dBm</span></div>\n      </div>\n    </div>\n\n    <div class=\"kpi\">\n      <div class=\"t\">UPTIME</div>\n      <div class=\"v\"><span class=\"mono\">{{state.uptime || '--'}}</span></div>\n    </div>\n\n    <div class=\"kpi\">\n      <div class=\"t\">LAST SEEN</div>\n      <div class=\"v\"><span class=\"mono\">{{state.lastSeen || '--'}}</span></div>\n    </div>\n\n    <div class=\"kpi\">\n      <div class=\"t\">LAST COMMAND</div>\n      <div class=\"v\"><span class=\"mono\">{{state.lastCommand || '--'}}</span></div>\n    </div>\n\n  </div>\n  \n  <!-- Thresholds Section -->\n  <div class=\"v360-section\">\n    <div class=\"v360-section-title\">⚙️ Current Thresholds (from device)</div>\n    <div class=\"threshold-grid\">\n      <div class=\"threshold-item\">\n        <div class=\"label\">Temp Min</div>\n        <div class=\"value\">{{state.thresholds.tempMin || 18}}°C</div>\n      </div>\n      <div class=\"threshold-item\">\n        <div class=\"label\">Temp Max</div>\n        <div class=\"value\">{{state.thresholds.tempMax || 30}}°C</div>\n      </div>\n      <div class=\"threshold-item\">\n        <div class=\"label\">Humidity Min</div>\n        <div class=\"value\">{{state.thresholds.humMin || 30}}%</div>\n      </div>\n      <div class=\"threshold-item\">\n        <div class=\"label\">Humidity Max</div>\n        <div class=\"value\">{{state.thresholds.humMax || 70}}%</div>\n      </div>\n      <div class=\"threshold-item\">\n        <div class=\"label\">Dust High</div>\n        <div class=\"value\">{{state.thresholds.dustHigh || 400}}</div>\n      </div>\n      <div class=\"threshold-item\">\n        <div class=\"label\">Gas High</div>\n        <div class=\"value\">{{state.thresholds.mq2High || 60}}</div>\n      </div>\n    </div>\n  </div>\n\n</div>\n\n<script>\n(function(scope){\n  scope.state = scope.state || { thresholds: {} };\n\n  scope.$watch('msg', function(msg){\n    if(!msg || !msg.payload) return;\n    scope.state = Object.assign({}, scope.state, msg.payload);\n    if (!scope.state.thresholds) scope.state.thresholds = {};\n  });\n\n  scope.toggleBuzzer = function(){\n    var desired = (scope.state.buzzer ? 'OFF' : 'ON');\n    scope.send({ topic: 'cmd/buzzer', payload: desired });\n  };\n})(scope);\n</script>",
